{
  "name": "Lovat",
  "version": "0.0.1",
  "description": "Example project using webpack and gradle",
  "scripts": {
    "devbuild": "gradle stage && npm run restart-server",
    "build": "webpack -p",
    "restart-server": "pkill -f heroku; heroku local",
    "watch": "npm run restart-server & onchange 'src/main/java/**/*' 'src/main/resources/assets/styles.css' 'src/main/resources/com/**/*' 'frontend/**/*' -- npm run devbuild"
  },
  "devDependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bufferutil": "^3.0.2",
    "material-ui": "^0.18.0",
    "onchange": "^3.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "utf-8-validate": "^3.0.3",
    "watch": "^1.0.2",
    "webpack": "^2.5.0",
    "ws": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damorton/lovat"
  },
  "license": "MIT"
}
